        android:theme="@style/Theme.AppF1Proyecto"
        tools:targetApi="31">


        <!-- Declaración de la actividad principal -->
        <activity
            android:name=".Pantalla1"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
