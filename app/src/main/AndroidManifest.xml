            </intent-filter>
        </activity>

        <!-- Declaración de otras actividades -->
        <activity android:name=".Pantalla2" android:exported="true" />
        <activity android:name=".Pantalla3" android:exported="true" />
        <activity android:name=".Pantalla4" android:exported="true" />
